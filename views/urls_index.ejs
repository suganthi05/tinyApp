<!DOCTYPE html>
<html lang ="en">
<head>

</head>
<body>
	<% include partials/_header %>
	<table>
	  <tr>
		  <td style="border-style: double"><b>Long URL</b></td>
		  <td style="border-style: double"><b>Short URL</b></td>
		  <td style="border-style: double"><b>Edit URL</b></td>
		  <td style="border-style: double"><b>Delete URL</b></td>
	  </tr>
	  <% Object.keys(urls).forEach(function(shortURL) { %>
	  <tr>
		  <td style="border-style: double"><a href="<%= urls[shortURL].url %>" target="_blank"><%= urls[shortURL].url %></a></td>
		  <td style="border-style: double"><a href="/u/<%= shortURL %>" target="_blank"><%= shortURL %></a></td>
		  <td style="border-style: double">
		  	<form action="/urls/<%= shortURL %>">
		  		<input type="submit" value="Edit">
		  	</form>
		  </td>
		  <td style="border-style: double">
		  	<form action="/urls/<%= shortURL %>/delete"method="POST"><input type="submit" value="Delete"></td>
		  	</form>
	  </tr>
	  <% }); %>
	</table>

</body>
</html>